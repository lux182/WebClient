}

function translate_errnum(num)
{
	var errnum = (num & 268435455); // get rid of severity
	var msg = "" + errnum.toString(16).toUpperCase();
	while (msg.length < 6)
	{
		msg = "0" + msg;
	}
		return "0x80" + msg;
}

